{
    "compileOnSave": false,
    "compilerOptions": {
        "outDir": "./obj/js",
        "sourceMap": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        "checkJs": true
    },
     "include": [
      "./Client/**/*.ts",
      "./Client/**/*.tsx",
      "./Client/**/*.jsx"
    ],
   "exclude": [
      "/**/node_modules/*"
    ]
}